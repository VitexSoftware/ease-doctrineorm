#!/bin/sh
# postinst script for php-vitexsoftware-ease-doctrineorm

set -e

case "$1" in
    configure)
        # Register the package with the Debian PHP system
        if [ -x /usr/sbin/phpenmod ]; then
            # Future use if we add any PHP modules
            true
        fi
        ;;

    abort-upgrade|abort-remove|abort-deconfigure)
        ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
        ;;
esac

#DEBHELPER#

exit 0